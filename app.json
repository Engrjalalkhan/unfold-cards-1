{
  "expo": {
    "name": "Unfold Cards",
    "slug": "unfold-cards",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#8B5CF6",
          "defaultChannel": "default"
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
      "backgroundImage": "./assets/logo.png",
      "backgroundImageResizeMode": "cover",
      "backgroundImageBlurRadius": 30,
      "imageStyle": {
        "width": 150,
        "height": 150,
        "borderRadius": 75,
        "overflow": "hidden"
      },
      "animation": {
        "wave": {
          "type": "wave",
          "duration": 2000,
          "easing": "easeInOut",
          "loop": true,
          "waveCount": 3,
          "waveAmplitude": 15,
          "wavePhase": 0.3
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "imageStyle": {
          "width": 150,
          "height": 150,
          "borderRadius": 75,
          "overflow": "hidden"
        }
      }
    },
    "android": {
      "package": "com.rafeeque2.unfoldcards",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "572808569364"
      },
      "firebase": {
        "projectId": "unfold-cards-8f621",
        "apiKey": "AIzaSyCNECdSoMOcmMseymPO6f1HJ6OeSTuQCGg",
        "messagingSenderId": "572808569364",
        "appId": "1:572808569364:android:d42c51ca0ac93dd070fd01"
      }
    }
  }
}
